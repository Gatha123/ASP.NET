 class Program
    {
        static void Main(string[] args)
        {
            
       char ch;
       
        Console.WriteLine("Enter any alphabet");
            ch = Convert.ToChar(Console.ReadLine());
            if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u')
                Console.WriteLine(ch + " is vowel");
            else
                Console.WriteLine(ch + " is consonant");
            Console.ReadKey();
        }
    }
}
    

