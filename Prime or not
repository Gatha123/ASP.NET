 
        int no,count = 0;
       
        Console.WriteLine("Enter any number");
            no = Convert.ToInt32(Console.ReadLine());
            for (int i = 1; i <= no; i++)
            {
                if (no % i == 0)
                    count++;
            }
            if (count == 2)
                Console.WriteLine(no + " is prime");
            else
                Console.WriteLine(no + " is not prime");

            Console.ReadKey();
