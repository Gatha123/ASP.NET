using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
namespace ConsoleApplication1
{
class Program
{
static void Main(string[] args)
{
Console.WriteLine("enter the limit");
int n = Convert.ToInt32(Console.ReadLine());
int b = Convert.ToInt32(Console.ReadLine());
int a = 0;
int b = 1;
int c = 0;
Console.WriteLine(a);
Console.WriteLine(b);
while (c <= n)
{
c = a + b;
a = b;
b = c;
Console.WriteLine(c);
}
Console.ReadKey();
}
}
}
