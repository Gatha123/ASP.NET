using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
namespace ConsoleApplication1
{
class Program
{
static void Main(string[] args)
{
int[] a = new int[5] { 1, 2, 3, 4, 5 };
Console.WriteLine("Array element are");
for(int i=0;i<5;i++)
{ Console.WriteLine(a[i]);
}
Console.ReadKey();
}
}
}
